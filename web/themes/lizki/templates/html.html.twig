<!doctype html>
{%
  set body_classes = [
    logged_in ? 'user-logged-in' : 'user-logged-out',
    not root_path ? 'path-frontpage' : 'path-' ~ root_path|clean_class,
    node_type ? 'page-node-type-' ~ node_type|clean_class,
    db_offline ? 'db-offline',
    current_path ? 'page' ~ current_path|clean_class,
  ]
%}
<html{{ html_attributes }}>
  <head>
    <head-placeholder token="{{ placeholder_token }}">
      <title>{{ head_title|safe_join(' | ') }}</title>
      
      <meta charset="utf-8" />
      <meta http-equiv="x-ua-compatible" content="ie-edge">
      <meta name="viewport" content="width=device-width, initial-scale=1, initial-scale=1.0, minimum-scale=1.0, shrink-to-fit=no, user-scalable=yes">
      <css-placeholder token="{{ placeholder_token }}">
      <js-placeholder token="{{ placeholder_token }}">
  </head>

  <body id="non-iframe-body" {{ attributes.addClass(body_classes) }}>
    <div class="dialog-off-canvas-main-canvas mil-wrapper" id="smooth-wrapper">
      <a id="mil-btt" class="mil-back-to-top"><i class="fas fa-arrow-up"></i></a>
        {{ page_top }}
        {{ page }}
        {{ page_bottom }}
    </div>
        <js-bottom-placeholder token="{{ placeholder_token }}">
  </body>
</html>